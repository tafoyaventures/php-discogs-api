    "type": "library",
    "keywords": ["discogs", "api"],
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "guzzlehttp/guzzle": "~7.0",
        "guzzlehttp/guzzle-services": "~1.2.0",
        "guzzlehttp/oauth-subscriber": "~0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5"
    },
    "authors": [
        {
            "name": "Richard van den Brand",
            "email": "richard@vandenbrand.org"
        },
        {
            "name": "Brian Tafoya",
            "email": "btafoya@tafoyaventures.com"
        }
    ],
    "autoload": {
        "psr-0": { "Discogs": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "ricbra/discogs-bundle": "For integrating Discogs into Symfony2"
    },
    "scripts":{
        "test": "phpunit tests"
    }
}
